  "homepage": "https://github.com/mourner/rbush",
  "keywords": [
    "spatial",
    "tree",
    "search",
    "rectangle",
    "index",
    "math"
  ],
  "author": "Vladimir Agafonkin",
  "repository": {
    "type": "git",
    "url": "git://github.com/mourner/rbush.git"
  },
  "main": "rbush.js",
  "devDependencies": {
    "benchmark": "^1.0.0",
    "faucet": "0.0.1",
    "istanbul": "~0.1.45",
    "jshint": "^2.5.0",
    "rtree": "~1.2.3",
    "tape": "^2.12.3"
  },
  "scripts": {
    "test": "jshint rbush.js test/test.js && node test/test.js | faucet",
    "perf": "node ./debug/perf.js",
    "cov": "istanbul cover test/test.js -x test/test.js"
  },
  "jshintConfig": {
    "quotmark": "single",
    "trailing": true
  }
}
